#cafe_show
  .image_holding_div
    = image_tag @cafe.image.url(:medium)
    .cafe_title
      Cafe Ambience
      %br
      %p
        Today's cafe from
        = @cafe.name

    / .other_test
    /   %h3=  @cafe.name
    /   %p= @cafe.description


    .white_logo_image
    .second_background_image
      / = render partial: 'player'

      .cafe_info
        %p= @cafe.name
        %p= @cafe.description
        %p= @cafe.street_address
        %p= @cafe.state
        %p= @cafe.editors_note

        %p= @cafe.website
        %p= @cafe.yelp_link



    / = link_to 'All Cafes ', cafes_path
    / = link_to 'Edit', edit_cafe_path(@cafe)
    / = link_to 'Delete', cafe_path(@cafe), method: :delete, data: { confirm: 'Are you sure?' }

